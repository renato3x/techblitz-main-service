generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String   @id @default(uuid())
  name       String
  username   String   @unique
  email      String   @unique
  password   String
  bio        String?
  avatar_url String?
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  @@map("users")
}
